{
  "version": 2,
  "builds": [
    {
      "src": "backend/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/index.js"
    }
  ],
  "env": {
    "BITSO_APIKEY": "@bitso_apikey",
    "BITSO_SECRET_APIKEY": "@bitso_secret_apikey"
  }
} 